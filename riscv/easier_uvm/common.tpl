# Mandatory settings
dut_top   = toplevel

# Instance name of generated DUT module. Default is uut
dut_iname = toplevel

# Suppress the generated file header that includes the above fields
generate_file_header = no
comments_at_include_locations = no

# Instantiates agent configuration objects from top-level config object. Default is no
nested_config_objects = yes

top_default_sequence_count = 10
